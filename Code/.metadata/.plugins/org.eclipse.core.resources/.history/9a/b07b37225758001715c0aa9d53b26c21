package net.kerfuffle.LitterBox;

import net.kerfuffle.Utilities.GUI.Quad;

public class Entity {

	private float speed;
	protected Quad box;
	
	public Entity(Quad box, float speed)
	{
		this.box=box;
		this.speed = speed;
	}
	
	public void moveLeft()
	{
		box.x -= speed;
	}
	public void moveRight()
	{
		box.x += speed;
	}
	public void moveUp()
	{
		box.y+=speed;
	}
	public void moveDown()
	{
		box.y-=speed;
	}
	
}
